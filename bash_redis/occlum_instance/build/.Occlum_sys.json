{
  "resource_limits": {
    "user_space_size": "600MB"
  },
  "process": {
    "default_stack_size": "4MB",
    "default_heap_size": "32MB",
    "default_mmap_size": "100MB"
  },
  "entry_points": [
    "/bin"
  ],
  "env": {
    "default": [
      "OCCLUM=yes"
    ],
    "untrusted": [
      "EXAMPLE"
    ]
  },
  "mount": [
    {
      "options": {
        "layers": [
          {
            "options": {
              "MAC": "7e-8f-87-67-a4-1c-23-af-d6-c5-b0-d6-a3-3d-0b-f8"
            },
            "source": "./build/initfs/__ROOT",
            "target": "/",
            "type": "sefs"
          },
          {
            "source": "./run/initfs/__ROOT",
            "target": "/",
            "type": "sefs"
          }
        ]
      },
      "target": "/",
      "type": "unionfs"
    },
    {
      "target": "/proc",
      "type": "procfs"
    },
    {
      "target": "/dev",
      "type": "devfs"
    }
  ]
}